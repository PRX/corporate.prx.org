<nav id=prx-main-nav>
  <ul>
    {.repeated section items}
      <li class="{.section active}active {.end}{.section externalLink}ext-link{.end}">
        {.section collection}
          <a href="{fullUrl}">{navigationTitle}</a>
        {.end}

        {.section externalLink}
          <a href="{url}">{title}</a>
        {.end}
      </li>
    {.end}
  </ul>
</nav>
