<!doctype html>
<html lang=en>
  <head>
    <meta charset=utf-8>
    <meta http-equiv=x-ua-compatible content="ie=edge">
    <meta name=viewport content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <meta name=description content=PRX>

    <link rel=alternate type=application/rss+xml title="PRX Blog" href=https://medium.com/feed/public-radio-exchange>

    <link href="https://fonts.googleapis.com/css?family=Alegreya|Montserrat:300,500,700|Open+Sans:300,400,400i,700,700i" rel="stylesheet">

    {@|apply google-tag-manager.block}

    <title>PRX</title>

    <squarespace:script src=main.js combo=false />
    <squarespace:script src=activate-links.js combo=false />

    {squarespace-headers}
  </head>
  <body class="{squarespace.page-classes} {.equal?:collection.urlId:"home"}prx-home{.end}" id="{squarespace.page-id}">
    {@|apply google-tag-manager-noscript.block}

    {@|apply nav-group.block}
