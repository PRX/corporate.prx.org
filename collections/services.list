{.section collection}
  {.section collections}
    <nav class="Index-nav">
      <div class="Index-nav-inner">
        {.repeated section @}
          <a href="#{urlId}" class="Index-nav-item{.equal? @index 1} active{.end}">
            <div class="Index-nav-indicator"></div>
            <div class="Index-nav-text"><span>{title}</span></div>
          </a>
        {.end}
      </div>
    </nav>
  {.end}

  {.repeated section collections}
    <div>
      {mainContent}
    </div>
  {.end}
{.end}
